{
	"env": {
		"es6": true,
		"node": true
	},
	"rules": {
		"accessor-pairs": 2,
		"array-bracket-spacing": [2, "never"],
		"arrow-parens": [2, "always"],
		"arrow-spacing": 2,
		"block-scoped-var": 2,
		"block-spacing": [2, "always"],
		"brace-style": [2, "1tbs"],
		"callback-return": 2,
		"camelcase": 1,
		"comma-dangle": 2,
		"comma-spacing": 2,
		"comma-style": [2, "last"],
		"complexity": [2, 10],
		"computed-property-spacing": [2, "never"],
		"consistent-return": 2,
		"consistent-this": [2, "self"],
		"curly": 2,
		"default-case": 2,
		"dot-location": [2, "property"],
		"dot-notation": 1,
		"eol-last": 2,
		"eqeqeq": [2, "smart"],
		"func-names": 2,
		"func-style": [2, "declaration", {
			"allowArrowFunctions": true
		}],
		"guard-for-in": 2,
		"handle-callback-err": 2,
		"id-length": [2, {
			"min": 2,
			"max": 16,
			"properties": "always",
			"exceptions": ["x", "y", "i", "j", "_"]
		}],
		"indent": [2, 4],
		"keyword-spacing": [2, {
			"before": true,
			"after": true,
			"overrides": {}
		}],
		"linebreak-style": [2, "unix"],
		"max-depth": [2, 3],
		"max-statements": [2, 15],
		"max-len": [1, 120],
		"max-nested-callbacks": [1, 4],
		"new-cap": [2, {
			"capIsNewExceptions": ["Sequelize.STRING",
				"Sequelize.INTEGER",
				"Sequelize.BIGINT",
				"Sequelize.FLOAT",
				"Sequelize.REAL",
				"Sequelize.DOUBLE",
				"Sequelize.DECIMAL",
				"Sequelize.ENUM",
				"Sequelize.ARRAY",
				"Sequelize.BLOB"
			]
		}],
		"no-alert": 2,
		"no-caller": 2,
		"no-catch-shadow": 2,
		"no-cond-assign": 2,
		"no-console": 2,
		"no-constant-condition": 2,
		"no-debugger": 2,
		"no-delete-var": 2,
		"no-div-regex": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-else-return": 2,
		"no-empty": 2,
		"no-empty-character-class": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-ex-assign": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 2,
		"no-extra-semi": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-func-assign": 2,
		"no-implicit-coercion": [2, {
			"boolean": true,
			"number": true,
			"string": false
		}],
		"no-implied-eval": 2,
		"no-inner-declarations": [2, "both"],
		"no-invalid-regexp": 2,
		"no-invalid-this": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 2,
		"no-label-var": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-lonely-if": 2,
		"no-loop-func": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-negated-in-lhs": 2,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-require": 2,
		"no-new-wrappers": 2,
		"no-obj-calls": 2,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-param-reassign": 0,
		"no-path-concat": 2,
		"no-process-env": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-regex-spaces": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-shadow": 2,
		"no-shadow-restricted-names": 2,
		"no-sparse-arrays": 2,
		"no-sync": 2,
		"no-throw-literal": 2,
		"no-trailing-spaces": [1, {
			"skipBlankLines": true
		}],
		"no-use-before-define": [2, "nofunc"],
		"no-undef": 2,
		"no-unexpected-multiline": 2,
		"no-unreachable": 2,
		"no-unused-expressions": 2,
		"no-unused-vars": 1,
		"no-useless-call": 1,
		"no-useless-concat": 2,
		"no-var": 2,
		"no-void": 2,
		"no-warning-comments": [1, {
			"location": "anywhere"
		}],
		"no-with": 2,
		"object-curly-spacing": [2, "never"],
		"prefer-arrow-callback": 2,
		"prefer-const": 2,
		"prefer-template": 2,
		"quotes": [2, "single", "avoid-escape"],
		"radix": 2,
		"require-jsdoc": 2,
		"semi": [2, "always"],
		"space-infix-ops": 2,
		"space-unary-ops": 2,
		"strict": [2, "global"],
		"use-isnan": 2,
		"valid-jsdoc": [2, {
			"prefer": {
				"arg": "param",
				"argument": "param",
				"constant": "const",
				"constructor": "class",
				"defaultvalue": "default",
				"desc": "description",
				"exception": "throws",
				"extends": "augments",
				"file": "overview",
				"fileoverview": "overview",
				"fires": "emits",
				"func": "function",
				"host": "external",
				"method": "function",
				"prop": "property",
				"return": "returns",
				"var": "member",
				"virtual": "abstract"
			},
			"requireReturn": false
		}],
		"valid-typeof": 2,
		"wrap-iife": [2, "inside"],
		"yoda": 2
	}
}
